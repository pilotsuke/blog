<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.test.model.mapper.UserMapper"  >
	
	<insert id="addUser" parameterType="UserInfo"  >
		insert into userinfo(username,age,email,birthday)
		values(#{username},#{age},#{email},#{birthday})
	</insert>
	
</mapper>










